{
	"name": "Dataflow4",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_ict_excel",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_synapse_ICT",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "flatten1"
				},
				{
					"name": "flatten2"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Task ID} as string,",
				"          {Task Name} as string,",
				"          {Bucket Name} as string,",
				"          Progress as string,",
				"          Priority as string,",
				"          {Assigned To} as string,",
				"          {Created By} as string,",
				"          {Created Date} as string,",
				"          {Start Date} as string,",
				"          {Due Date} as string,",
				"          Late as string,",
				"          {Completed Date} as string,",
				"          {Completed By} as string,",
				"          Description as string,",
				"          {Completed Checklist Items} as string,",
				"          {Checklist Items} as string,",
				"          Labels as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'Filename') ~> source1",
				"source1 derive(Assigned_name = split({Assigned To}, ';'),",
				"          Labels = split(Labels, ';')) ~> derivedColumn1",
				"derivedColumn1 foldDown(unroll(Assigned_name, Assigned_name),",
				"     mapColumn(",
				"          {Task ID},",
				"          {Task Name},",
				"          {Bucket Name},",
				"          Progress,",
				"          Priority,",
				"          {Assigned To},",
				"          {Created By},",
				"          {Created Date},",
				"          {Start Date},",
				"          {Due Date},",
				"          Late,",
				"          {Completed Date},",
				"          {Completed By},",
				"          Description,",
				"          {Completed Checklist Items},",
				"          {Checklist Items},",
				"          Labels,",
				"          Filename,",
				"          Assigned_name",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"flatten1 foldDown(unroll(Labels, Labels),",
				"     mapColumn(",
				"          {Task ID},",
				"          {Task Name},",
				"          {Bucket Name},",
				"          Progress,",
				"          Priority,",
				"          {Assigned To},",
				"          {Created By},",
				"          {Created Date},",
				"          {Start Date},",
				"          {Due Date},",
				"          Late,",
				"          {Completed Date},",
				"          {Completed By},",
				"          Description,",
				"          {Completed Checklist Items},",
				"          {Checklist Items},",
				"          Labels,",
				"          Filename,",
				"          Assigned_name",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten2",
				"flatten2 select(mapColumn(",
				"          Task_ID = {Task ID},",
				"          Task_Name = {Task Name},",
				"          Buckets = {Bucket Name},",
				"          Progress,",
				"          Priority,",
				"          Created_By = {Created By},",
				"          Created_Date = {Created Date},",
				"          Start_Date = {Start Date},",
				"          Due_Date = {Due Date},",
				"          Late,",
				"          Completed_Date = {Completed Date},",
				"          Completed_By = {Completed By},",
				"          Completed_Checklist_Items = {Completed Checklist Items},",
				"          Checklist_Items = {Checklist Items},",
				"          Labels,",
				"          Filename,",
				"          Assigned_name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Task_ID as string,",
				"          Task_Name as string,",
				"          Buckets as string,",
				"          Progress as string,",
				"          Priority as string,",
				"          Created_By as string,",
				"          Created_Date as string,",
				"          Start_Date as string,",
				"          Due_Date as string,",
				"          Late as string,",
				"          Completed_Date as string,",
				"          Completed_By as string,",
				"          Completed_Checklist_Items as string,",
				"          Checklist_items as string,",
				"          Assigned_name as string,",
				"          Labels as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}